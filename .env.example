# Snaxel Query Engine Configuration

# Browser Configuration
PUPPETEER_TIMEOUT=30000
PUPPETEER_MAX_RETRIES=3
PUPPETEER_RETRY_DELAY=1000

# Concurrency
MAX_CONCURRENT_SCRAPERS=3

# User Agent (optional, uses default if not set)
# USER_AGENT="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"

# Proxy Configuration (optional)
# PROXY_HOST=
# PROXY_PORT=
# PROXY_USERNAME=
# PROXY_PASSWORD=

# Rate Limiting (requests per minute)
RATE_LIMIT=60

# Cache Configuration (optional)
# ENABLE_CACHE=true
# CACHE_TTL=3600

# Logging
LOG_LEVEL=info
# LOG_FILE=./logs/snaxel.log

# API Keys (if using fallback APIs)
# BING_API_KEY=
# YOUTUBE_API_KEY=